interface 'List {
    'head : to Any
    'tail : to List
    ', : Any to List
}

class 'List {
    'car : Any
    'cdr : List
    'head := {
        car return
    }
    'tail := {
        cdr return
    }
    'List := {
        this car := (arg 0)
        this cdr := (arg 1 as List)
    }
    ', := {
        new List (arg 0) (this)
    }
}
